{
    "name": "Rich Diff",
    "description": "open rich diff when reviewing a pull request",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "128": "./logo.png"
    },
    "background":{
        "scripts":["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Rich Diff"
    },
    "permissions": ["tabs", "https://github.com/*"]
}
